<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <layer-list>
            <item>
                <shape>
                    <size
                        android:width="29dp"
                        android:height="24dp" />
                    <solid android:color="#00000000" />
                    <padding
                        android:left="7dp"
                        android:top="2dp"
                        android:right="2dp"
                        android:bottom="2dp" />
                </shape>
            </item>
            <item>
                <shape android:shape="oval">
                    <solid android:color="@color/colorCheckboxDefaultSelected" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_checked="false">
        <layer-list>
            <item>
                <shape>
                    <size
                        android:width="29dp"
                        android:height="24dp" />
                    <solid android:color="#00000000" />
                    <padding
                        android:left="7dp"
                        android:top="2dp"
                        android:right="2dp"
                        android:bottom="2dp" />
                </shape>
            </item>
            <item>
                <shape android:shape="oval">
                    <solid android:color="@color/colorPrimaryButton" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
